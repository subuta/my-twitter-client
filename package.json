{
  "name": "my-twitter-client",
  "version": "1.0.0",
  "description": "Slack-like twitter client for sub-labo.com",
  "main": "index.js",
  "scripts": {
    "copy": "copyfiles -u 1 src/**/*.graphql lib",
    "watch:css": "DEBUG=css-as-js:* css-as-js",
    "build:css": "NODE_ENV=production DEBUG=css-as-js:* css-as-js",
    "build:server": "npm run copy && NODE_ENV=production rur build",
    "build": "run-p build:css build:server ",
    "pdeploy": "cd prisma && prisma deploy",
    "serve:server": "rur start",
    "serve": "run-p watch:css serve:server",
    "svg": "svg-sprite -C ./svg-sprite-config.json 'src/assets/svg/*.svg'",
    "playground": "graphql playground --port 3001",
    "servep": "NODE_PATH=./ NODE_ENV=production rur start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subuta/play-with-rollup.git"
  },
  "author": "subuta <shunta.3110@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/subuta/play-with-rollup/issues"
  },
  "jest": {
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/scripts/jest.setup.js"
  },
  "homepage": "https://github.com/subuta/play-with-rollup#readme",
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "apollo-server-koa": "^1.4.0",
    "async-exit-hook": "^2.0.1",
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "boom": "^7.2.0",
    "css-as-js": "^0.1.10",
    "dotenv": "^6.1.0",
    "emoji-js": "^3.4.1",
    "esm": "^3.0.84",
    "graphql": "0.13.0",
    "graphql-import": "^0.7.1",
    "graphql-request": "^1.8.2",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.0",
    "graphql-type-json": "^0.2.1",
    "icepick": "^2.3.1",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.5.3",
    "koa-body": "^4.0.4",
    "koa-logger": "^3.2.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "loadable-components": "^2.2.3",
    "lodash": "^4.17.11",
    "lru-cache": "^4.1.3",
    "moment": "^2.22.2",
    "p-min-delay": "^2.0.0",
