    "react-renderless-virtual-list": "^0.1.10",
    "react-router-dom": "^4.3.1",
    "react-tiny-virtual-list": "^2.2.0",
    "react-ur": "^0.1.15",
    "react-ur-tools": "^0.1.15",
    "recompose": "^0.30.0",
